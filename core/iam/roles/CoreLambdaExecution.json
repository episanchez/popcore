{
  "description": "Allow Lambda to write in CloudWatch",
  "managed-policies": ["AWSLambdaBasicExecutionRole", "AmazonVPCFullAccess", "AmazonRDSFullAccess"],
  "inline-policies": [],
  "trust-relationship": {
    "Version": "2012-10-17",
    "Statement": [{
      "Effect": "Allow",
      "Principal": {
        "Service": "lambda.amazonaws.com"
      },
      "Action": "sts:AssumeRole"
    }]
  }
}
